APP_NAME=FrozenFood
APP_ENV=production
APP_KEY=base64:your_app_key_will_be_generated
APP_DEBUG=false
APP_URL=https://your-backend-domain.com

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=error

DB_CONNECTION=mysql
DB_HOST=your-production-db-host
DB_PORT=3306
DB_DATABASE=frozen_food_db
DB_USERNAME=your-production-username
DB_PASSWORD=your-production-password

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

# Update these with your GitHub Pages URL
SANCTUM_STATEFUL_DOMAINS=rzandi.github.io
SESSION_DOMAIN=rzandi.github.io
CORS_ALLOWED_ORIGINS=https://rzandi.github.io